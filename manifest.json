{
  "manifest_version": 3,
  "name": "TabForge AI",
  "version": "1.0",
  "description": "AI-powered tab organization",
  "permissions": [
    "tabs",
    "tabGroups",
    "storage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Organize Tabs with AI"
  }
}