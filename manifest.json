{
  "manifest_version": 3,
  "name": "Tabby - Smart Tab Organizer",
  "version": "1.0",
  "description": "Simple and reliable tab organization with auto-cleanup",
  "permissions": ["tabs", "tabGroups", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/Icon.png",
    "48": "icons/Icon.png", 
    "128": "icons/Icon.png"
  }
}