{
  "manifest_version": 3,
  "name": "TabForge AI",
  "version": "1.0",
  "description": "AI-powered tab organization with themes",
  "permissions": ["tabs", "tabGroups", "storage", "notifications"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}